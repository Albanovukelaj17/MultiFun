<Window xmlns="https://github.com/avaloniaui"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
x:Class="TicTacToe.ttt_Window"
Title="Tic Tac Toe" Width="300" Height="300">
    <Grid>
    <Grid.RowDefinitions>
    <RowDefinition Height="*" />
    <RowDefinition Height="*" />
    <RowDefinition Height="*" />
    </Grid.RowDefinitions>
    <Grid.ColumnDefinitions>
    <ColumnDefinition Width="*" />
    <ColumnDefinition Width="*" />
    <ColumnDefinition Width="*" />
    </Grid.ColumnDefinitions>
        
    <Button Grid.Row="0" Grid.Column="0" Name="Button00" Click="OnCellClick"/>
    <Button Grid.Row="0" Grid.Column="1" Name="Button01" Click="OnCellClick"/>
    <Button Grid.Row="0" Grid.Column="2" Name="Button02" Click="OnCellClick"/>
        
    <Button Grid.Row="1" Grid.Column="0" Name="Button10" Click="OnCellClick"/>
    <Button Grid.Row="1" Grid.Column="1" Name="Button11" Click="OnCellClick"/>
    <Button Grid.Row="1" Grid.Column="2" Name="Button12" Click="OnCellClick"/>
        
    <Button Grid.Row="2" Grid.Column="0" Name="Button20" Click="OnCellClick"/>
    <Button Grid.Row="2" Grid.Column="1" Name="Button21" Click="OnCellClick"/>
    <Button Grid.Row="2" Grid.Column="2" Name="Button22" Click="OnCellClick"/>
    </Grid>
    </Window>